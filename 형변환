#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main(void)
{
	int i;
	double f;

	f = 5 / 4;
	//5/4는 1이 되고 이것이 1.0이 된다.

	printf("%f\n", f);

	f = (double)5 / 4;
	printf("%f\n", f);
	//5가 5.0으로 되어서 전체 결과가 1.25가 된다.

	f = 5.0 / 4;
	printf("%f\n", f);
	
	f = (double)5 / (double)4;
	printf("%f/n", f);

	i = 1.3 + 1.8;
	printf("%d\n", i);

	i = (int)1.3 + (int)1.8;
	//1.3이 1이 되고 1.8도 1이 되어서 최종 결과는 2가 된다.

	printf("%d\n", i);

	return 0;
}
